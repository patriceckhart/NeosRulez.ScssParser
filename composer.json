{
    "description": "A package to compile SCSS for Neos CMS",
    "license": "GPL-3.0-or-later",
    "type": "neos-plugin",
    "name": "neosrulez/scssparser",
    "authors": [
        {
            "name": "Patric Eckhart",
            "email": "mail@patriceckhart.com",
            "homepage": "http://www.patriceckhart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "*",
        "neos/nodetypes": "*",
        "leafo/scssphp": "0.8.4"
    },
    "autoload": {
        "psr-4": {
            "Leafo\\ScssPhp\\": "lib/overrides/leafo/scssphp/src/",
            "NeosRulez\\ScssParser\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "NeosRulez.ScssParser"
        }
    }
}
