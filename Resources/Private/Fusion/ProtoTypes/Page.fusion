prototype(NeosRulez.ScssParser:ScssFile) < prototype(Neos.Neos:Plugin)
prototype(NeosRulez.ScssParser:ScssFile) {
    package = 'NeosRulez.ScssParser'
    controller = 'ScssParser'
    action = 'includeFile'
    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}